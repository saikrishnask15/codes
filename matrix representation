#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int t;
    scanf("%d",&t);
    while(t--){
        int n,a[32];
        scanf("%d",&n);
        int i=0;
        if(n==0){
            printf("0");
        }
        while(n>0){
            a[i]=n%2;
            n=n/2;
            i++;
        }
        for(int j=i-1;j>=0;j--){
            printf("%d",a[j]);
        }
         printf("\n");
    }
   
    /* Enter your cod here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
